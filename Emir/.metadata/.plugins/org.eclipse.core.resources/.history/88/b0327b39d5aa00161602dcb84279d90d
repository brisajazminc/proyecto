		case 1: 
			registrar();
			break;
		}
		
		ResultSet resultado;
		String nombres;
		
		resultado = conexion.getQuery("select * from cliente");
		
		try {
			while(resultado.next()){
				nombres = resultado.getString("Nombre");
				System.out.println("nombre: "+nombres);
			}
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}		
	}
	
